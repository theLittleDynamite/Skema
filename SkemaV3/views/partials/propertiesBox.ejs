<!-- views/partials/properties.ejs -->
<div class="properties">
    <h1>Properties</h1>

    <!-- Display the items in the Mongo databasee -->
    <!-- TODO: This is more for testing and space filler - not final -->
    <ul class="graph">
        <li>View: <%= view.name %>
        <% for (var one_node of view.nodes) { %>
            <li>Node: <%= one_node %></li>
        <% } %>
        <% for (var one_edge of view.edges) { %>
            <li>Edge: <%= one_edge %></li>
        <% } %>
    </ul>
</div>
